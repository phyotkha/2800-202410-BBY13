<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width = device-width, initial-scale=1.0">
  <title>SchoolScope AI</title>
  <link href="/bby-13-favicon.ico" rel="icon" type="image/ico">
  <link rel="canonical" href="https://getbootstrap.com/docs/5.3/examples/navbars-offcanvas/">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
  <link href="/assets/css/calendar.css" rel="stylesheet" crossorigin="anonymous">
  <link href="/assets/css/style.css" rel="stylesheet">
  <!-- Syncfusion EJ2 Schedule CSS -->
  <link href="https://cdn.syncfusion.com/ej2/20.1.55/material.css" rel="stylesheet">
  <!-- Syncfusion EJ2 Base, Buttons, and Schedule JS -->
  <script src="https://cdn.syncfusion.com/ej2/20.1.55/dist/ej2.min.js"></script>
  <script>

    ej.base.registerLicense('<%= licenseKey %>');
    document.addEventListener('DOMContentLoaded', function () {
      const { Schedule, Day, Week, WorkWeek, Month, Agenda } = ej.schedule;
      let scheduleObj = new Schedule({
        height: '550px',
        selectedDate: new Date(),
        eventSettings: { dataSource: [] },
        allowAdding: false, 
        views: ['Day', 'Week', 'WorkWeek', 'Month', 'Agenda']
      });

      scheduleObj.appendTo('#schedule');
    });
  </script>
</head>

<body class="d-flex flex-column vh-100">
  <header class="header py-3">
    <nav
      class="navbar navbar-custom navbar-expand-md navbar-dark flex-wrap align-items-center justify-content-center py-3 mb-4 border-bottom fixed-top border-info transparent">
      <div class="container-fluid">
        <a href="/chatPage" class="navbar-brand"><img src="/navbar-logo-letter.png" alt="Logo" width="55"
            height="55"></a>
        <button class="navbar-toggler collapsed" type="button" data-bs-toggle="collapse"
          data-bs-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false"
          aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="navbar-collapse collapse" id="navbarCollapse">
        <ul
          class="navbar px-5 navbar-nav me-auto mb-2 nav-pills nav-fill align-items-center justify-content-center mb-md-0">
          <% for(var i=0; i<locals.navLinks.length; i++) { %>
            <li class="nav-item"><a href="<%= locals.navLinks[i].link %>"
                class="nav-link <% if (locals.navLinks[i].link == locals.currentURL) { %> .nav-underline <% } %>"
                aria-current="page">
                <%= locals.navLinks[i].name %>
              </a></li>
            <% } %>
        </ul>
        <div class="navbar-nav px-0.5 mb-2 align-items-center justify-content-end mb-md-0">
          <a href='/logout' method='get' class="nav-link"><button type="button"
              class="btn btn-outline-warning">Logout</button></a>
        </div>
      </div>
    </div>
   </nav>
  </header><br>
<div class="main mt-5">